<div class="form-container">

  <h4>Journal</h4>
  <i class="fas fa-lungs"></i>

  <%= simple_form_for @symptom_check do |f| %>
  <%= f.input :rating, label: 'How is your cough today?', as: :range, input_html: { min: 1, max: 10 }, wrapper_html: { id: 'rating' }, required: false %>
  <%= f.input :description %>
  <%= f.button :submit, value: 'Submit', class: 'btn-ghost' %>
  <% end %>
</div>
